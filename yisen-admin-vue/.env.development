# 开发环境配置
NODE_ENV=development

# 应用标题
VITE_APP_TITLE=益森管理系统

# API 基础路径
VITE_API_BASE_URL=http://localhost:8082/api

# API 前缀
VITE_API_PREFIX=/api

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE=10

# 是否开启 Mock 数据
VITE_USE_MOCK=true

# 是否显示调试信息
VITE_SHOW_DEBUG=true

# 端口号
VITE_PORT=8081

# 是否自动打开浏览器
VITE_OPEN=false

# 是否启用 HTTPS
VITE_HTTPS=false