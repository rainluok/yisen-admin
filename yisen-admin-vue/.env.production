NODE_ENV=production

# 应用标题
VITE_APP_TITLE=益森管理系统

# API 基础路径
VITE_API_BASE_URL=https://api.yisen.com

# API 前缀
VITE_API_PREFIX=/api

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE=10

# 是否开启 Mock 数据
VITE_USE_MOCK=false

# 是否显示调试信息
VITE_SHOW_DEBUG=false

# 是否启用压缩
VITE_BUILD_COMPRESS=gzip

# 是否删除 console
VITE_DROP_CONSOLE=true

# 是否生成源码映射
VITE_SOURCEMAP=false

# 输出目录
VITE_OUTPUT_DIR=dist